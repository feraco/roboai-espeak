{
  // Cloud-Only Configuration: Google Gemini + Google TTS + Google ASR
  // Google's ecosystem for integrated AI services
  
  "hertz": 1,
  "name": "GeminiAI-Cloud",
  "api_key": null,
  
  // System prompts
  "system_prompt_base": "You are GeminiAI, an advanced AI assistant powered by Google's Gemini model. You are knowledgeable, helpful, and provide thoughtful responses with natural conversation flow.",
  "system_governance": "Here are the laws that govern your actions. Do not violate these laws.\nFirst Law: A robot cannot harm a human or allow a human to come to harm.\nSecond Law: A robot must obey orders from humans, unless those orders conflict with the First Law.\nThird Law: A robot must protect itself, as long as that protection doesn't conflict with the First or Second Law.",
  "system_prompt_examples": "Here are some examples of interactions:\n\n1. If a person asks 'What's the latest in AI?', you might:\n    Speak: {'sentence': 'AI is rapidly evolving! I'm powered by Google's Gemini model, which represents some of the latest advances in language understanding.'}\n\n2. If a person says 'Good morning!', you might:\n    Speak: {'sentence': 'Good morning! I'm GeminiAI, ready to assist you with any questions or tasks you have today.'}",
  
  // Input configuration - Google ASR
  "agent_inputs": [
    {
      "type": "GoogleASR",
      "config": {
        "language_code": "en-US",
        "sample_rate": 16000,
        "encoding": "LINEAR16",
        "api_key_env": "GOOGLE_CLOUD_API_KEY"
      }
    }
  ],
  
  // LLM configuration - Google Gemini
  "cortex_llm": {
    "type": "GeminiLLM",
    "config": {
      "agent_name": "GeminiAI-Cloud",
      "model": "gemini-pro",
      "temperature": 0.7,
      "max_output_tokens": 1000,
      "history_length": 12
    }
  },
  
  // Action configuration - Google TTS (cloud)
  "agent_actions": [
    {
      "name": "speak",
      "llm_label": "speak",
      "implementation": "passthrough",
      "connector": "google_tts",
      "config": {
        "language_code": "en-US",
        "voice_name": "en-US-Wavenet-D",
        "speaking_rate": 1.0,
        "pitch": 0.0,
        "api_key_env": "GOOGLE_CLOUD_API_KEY"
      }
    }
  ],
  
  // Simulators (required but can be empty)
  "simulators": [],
  
  // Background processes (optional)
  "backgrounds": []
}