{
  name: "simple_face_test",
  api_key: null,
  hertz: 2,
  system_prompt_base: "You are testing face detection. When you detect a face, say 'I can see a person!' When no face is detected, stay quiet.",
  system_governance: "Respond only when face detection triggers.",
  system_prompt_examples: "Face detected response: {\"speak\": {\"sentence\": \"I can see someone in front of the camera!\", \"language\": \"en\"}}",
  agent_inputs: [
    {
      type: "FacePresence",
      config: {
        camera_index: 0,
        poll_interval: 2.0,
        detection_threshold: 0.3,
        descriptor: "Face Detection - Quick Test"
      }
    }
  ],
  cortex_llm: {
    type: "OllamaLLM", 
    config: {
      agent_name: "Face Tester",
      base_url: "http://localhost:11434",
      model: "gemma2:2b",
      temperature: 0.1,
      timeout: 30,
      max_tokens: 50,
      num_predict: 40,
      options: {
        temperature: 0.1,
        top_p: 0.9,
        repeat_penalty: 1.1
      }
    }
  },
  agent_actions: [
    {
      name: "speak",
      llm_label: "speak", 
      type: "Action",
      implementation: "passthrough",
      connector: "piper_tts",
      config: {
        model_en: "en_US-ryan-medium",
        output_dir: "audio_output",
        sample_rate: 22050,
        default_language: "en",
        volume: 1.0
      }
    }
  ],
  simulators: [],
  backgrounds: []
}